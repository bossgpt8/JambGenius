{
  "version": 2,
  "buildCommand": "cd JambGenius && npm run build:css",
  "installCommand": "cd JambGenius && npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/JambGenius/api/$1.js"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/JambGenius/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|json|woff|woff2|ttf|eot))",
      "dest": "/JambGenius/$1"
    },
    {
      "src": "/([^.]+)",
      "dest": "/JambGenius/$1.html"
    },
    {
      "src": "/",
      "dest": "/JambGenius/index.html"
    }
  ]
}
