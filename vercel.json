{
  "version": 2,
  "buildCommand": "cd JambGenius && npm run build:css",
  "installCommand": "cd JambGenius && npm install",
  "functions": {
    "JambGenius/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/JambGenius/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/JambGenius/$1"
    }
  ]
}
