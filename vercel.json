{
  "version": 2,
  "buildCommand": "npm --prefix JambGenius install && npm --prefix JambGenius run build:css",
  "functions": {
    "JambGenius/api/**/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/JambGenius/api"
    },
    {
      "src": "/(.*)",
      "dest": "/JambGenius/$1"
    }
  ]
}
